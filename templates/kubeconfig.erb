apiVersion: v1
kind: Config
clusters:
<%- @options['clusters'].each do |cluster,data| -%>
- name: <%= cluster %>
  cluster:
    <%- data.each do |setting| -%>
    <%= setting %>
    <%- end -%>
<%- end -%>
contexts:
<%- @options['contexts'].each do |context,data| -%>
- name: <%= context %>
  context:
    <%- data.each do |setting| -%>
    <%= setting %>
    <%- end -%>
<%- end -%>
current-context: <%= @options['current-context'] %>
users:
<%- @options['users'].each do |user,data| -%>
- name: <%= user %>
  user:
  <%- data.each do |setting| -%>
  <%= setting %>
  <%- end -%>
<%- end -%>
